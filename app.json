{
  "expo": {
    "name": "Splinx-Planet",
    "slug": "Splinx-Planet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/splinxicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splinxlogo2.png",
      "resizeMode": "contain",
      "backgroundColor": "#fdccbb"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splinxicon.png",
        "backgroundColor": "#fdccbb"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-localization",
     [
       "expo-contacts",
       {
         "contactsPermission": "Allow $(PRODUCT_NAME) to access your contacts."
       }
     ]
    ]
  }
}
